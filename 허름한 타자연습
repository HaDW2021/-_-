import random
import time
w = ['cat', 'dog', 'fox']
n = 1
print("[타자 게임] 준비 되면 엔터!")

input()

start = time.time()

q = random.choice(w)

while n <= 2:
    print(q)
    x = input()
    if q == x:
        print("통과!")
        n = n+1
        q= random.choice(w)
    
    else:
        
        print("오타! 다시 도전!")
        
end = time.time()
et = end - start
et = format(et, ".2f") 
print("타자 시간:", et, "초")
